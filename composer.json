{
    "name": "aswoole/db",
    "description": "A db component for swoole.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "swoole",
        "coroutine",
        "db"
    ],
    "authors": [
        {
            "name": "caixukun"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.9"
    },
    "autoload": {
        "psr-4": {
            "Aswoole\\DB\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    }
}